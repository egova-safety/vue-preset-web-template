{
    "name": "vue-preset-web-template",
    "version": "0.0.20",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "build:serve": "http-server ./dist -a 127.0.0.1 -p 5050",
        "template:clear": "node build/template.clear.js",
        "template:build": "node build/template.build.js",
        "template:deploy": "cd .template && git init && git add -A && git commit -m 'deploy' && git push -f https://github.com/egova-safety/vue-preset-web-template.git master:master",
        "template": "npm run template:clear && npm run template:build && npm run template:deploy",
        "dll": "vue-cli-service dll"
    },
    "dependencies": {
        "@egova/flagwind-core": "^1.1.2",
        "@egova/flagwind-echarts": "^1.0.6",
        "@egova/flagwind-web": "^1.0.19",
        "axios": "^0.18.0",
        "echarts": "^4.1.0",
        "equals": "^1.0.5",
        "iview": "^3.2.2",
        "js-cookie": "^2.2.0",
        "lodash.debounce": "^4.0.8",
        "vue": "^2.6.10",
        "vue-class-component": "^7.0.2",
        "vue-property-decorator": "^8.1.0",
        "vue-router": "^3.0.6",
        "vuex": "^3.1.0"
    },
    "devDependencies": {
        "@egova/vue-cli-plugin-dll": "^1.1.7",
        "@types/js-cookie": "^2.2.1",
        "@types/node": "^11.9.4",
        "@vue/cli-plugin-babel": "^3.4.0",
        "@vue/cli-plugin-typescript": "^3.4.0",
        "@vue/cli-service": "^3.7.0",
        "add-asset-html-webpack-plugin": "^3.1.3",
        "compression-webpack-plugin": "^2.0.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "raw-loader": "^1.0.0",
        "sass": "^1.17.0",
        "sass-loader": "^7.1.0",
        "style-resources-loader": "^1.2.1",
        "tslint-config-flagwind": "^1.0.1",
        "tslint-loader": "^3.5.4",
        "typescript": "^3.3.3",
        "vue-template-compiler": "^2.5.21",
        "webpack-cli": "^3.2.3"
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
